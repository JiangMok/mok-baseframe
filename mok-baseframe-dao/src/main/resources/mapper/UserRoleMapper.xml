<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.mok.baseframe.dao.UserRoleMapper">

    <insert id="insertBatch" parameterType="list">
        INSERT INTO sys_user_role
        (
        id,
        user_id,
        role_id,
        create_time
        )
        VALUES
        <foreach collection="list" item="userRoleEntityList" separator=",">
            (
            #{userRoleEntityList.id},
            #{userRoleEntityList.userId},
            #{userRoleEntityList.roleId},
            #{userRoleEntityList.createTime}
            )
        </foreach>
    </insert>
</mapper>